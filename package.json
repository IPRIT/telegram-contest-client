{
  "name": "contest-parser-client",
  "version": "1.0.0",
  "description": "Contest Parser",
  "author": "Alexander Belov 2019",
  "private": true,
  "scripts": {
    "build": "npm run build:modern",
    "build:legacy": "cross-env NODE_ENV=production nuxt build",
    "build:modern": "cross-env NODE_ENV=production nuxt build --modern",
    "build:spa": "cross-env NODE_ENV=production nuxt build --spa",
    "start": "npm run start:modern",
    "start:legacy": "cross-env PORT=8091 NODE_ENV=production nuxt start",
    "start:modern": "node ./node_modules/nuxt/bin/nuxt.js start --modern",
    "start:spa": "cross-env PORT=8081 NODE_ENV=production nuxt start --spa",
    "dev": "cross-env PORT=9000 NODE_ENV=development nuxt",
    "dev:modern": "cross-env PORT=9000 NODE_ENV=development nuxt --modern",
    "dev:spa": "cross-env PORT=9000 NODE_ENV=development nuxt --spa",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "predeploy": "npm run build",
    "predeploy:flexible": "npm run build",
    "deploy": "gcloud app deploy",
    "deploy:flexible": "gcloud app deploy app.flexible.yaml"
  },
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "@nuxtjs/axios": "^5.4.1",
    "@nuxtjs/component-cache": "^1.1.4",
    "@nuxtjs/moment": "^1.1.0",
    "@nuxtjs/pwa": "^2.6.0",
    "@nuxtjs/router": "^1.3.2",
    "@rabota/loader": "^1.0.2",
    "cookie-universal-nuxt": "^2.0.11",
    "event-source-polyfill": "^1.0.5",
    "lru-cache": "^5.1.1",
    "md-svg-vue": "^1.0.20",
    "normalize.css": "^8.0.1",
    "nuxt": "^2.4.5",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@nuxtjs/webpackmonitor": "^0.1.0",
    "@std/esm": "^0.26.0",
    "babel-eslint": "^10.0.1",
    "compass-mixins": "^0.12.10",
    "cross-env": "^5.2.0",
    "eslint": "^5.11.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-html": "^5.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^2.1.0",
    "node-sass": "^4.12.0",
    "sass-bem-utils": "^1.16.0",
    "sass-loader": "^7.1.0"
  }
}
